export async function getStaticPaths () {
    const posts = await Astro.glob('./posts/*.md');

    let tags = new Set();

    posts.forEach((post) => {
        post.frontmatter.tags.forEach((tag: string) => {
            tags.add(tag);
        })
    })

    let res: Array<Object> = []

    tags.forEach((tag) => {
        res.push({params: {tag: tag}});
    })

    return res;
}
